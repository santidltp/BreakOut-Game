<?xml version='1.0'?>
<b:Game xmlns:b="breakout" grid_x="30" grid_y="20">
	<b:blockTemplate id="red" width="30" height="20" score="50">
		<b:colorType R="200" G="5" B="5"/>
	</b:blockTemplate>
	
	<b:blockTemplate id="blue" width="30" height="20" score="50">
		<b:colorType R="5" G="5" B="200"/>
