	<b:level>
		<b:image file="graphics/blackness.png">
			<b:frame duration="0">
				<b:location x="0" y="0" width="100" height="100"></b:location>
			</b:frame>	
		</b:image>
		<b:blockPattern class="PatternSpiral" startx="13" starty="12" count="89">
			<b:block>red</b:block>
			<b:block>blue</b:block>
		</b:blockPattern>
	</b:level>
